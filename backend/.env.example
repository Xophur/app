# Backend Environment Variables
# Copy this to your backend .env file

# MongoDB - Replace with your MongoDB Atlas or Digital Ocean managed DB URL
MONGO_URL="mongodb+srv://username:password@cluster.mongodb.net/lovelife"
DB_NAME="lovelife_db"

# CORS - Set to your frontend URL in production
CORS_ORIGINS="https://your-frontend-domain.com,http://localhost:3000"

# Stripe - Get from https://dashboard.stripe.com/apikeys
STRIPE_API_KEY="sk_live_your_stripe_secret_key"
